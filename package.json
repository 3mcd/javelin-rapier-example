{
  "name": "javelin-bounce",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "concurrently -k \"npm:start:client\" \"npm:start:server\"",
    "start:client": "vite --config client/vite.config.ts",
    "start:server": "tsc -p server/tsconfig.json && node --es-module-specifier-resolution=node --experimental-wasm-modules server/dist/server/src/index.js",
    "debug": "concurrently -k \"npm:start:client\" \"npm:debug:server\"",
    "debug:server": "ndb ts-node server/index.ts"
  },
  "dependencies": {
    "@dimforge/rapier2d": "^0.5.3",
    "@javelin/ecs": "^1.0.0-alpha.0",
    "@javelin/hrtime-loop": "^0.22.0",
    "@javelin/net": "^1.0.0-alpha.0",
    "@javelin/pack": "^1.0.0-alpha.0",
    "@javelin/track": "^1.0.0-alpha.0",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "@web-udp/client": "^4.2.1",
    "@web-udp/server": "^4.2.1",
    "concurrently": "^6.1.0",
    "install": "^0.13.0",
    "planck": "^1.0.0-alpha.2",
    "pnpm": "^6.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.3.2"
  }
}
